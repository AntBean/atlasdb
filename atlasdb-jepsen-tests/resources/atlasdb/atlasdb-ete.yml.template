server:
  applicationConnectors:
    - type: http
      port: 3828
  adminConnectors:
    - type: http
      port: 3829
  requestLog:
    appenders:
      - type: file
        archive: false
        currentLogFilename: var/log/atlasdb-ete-request.log
        threshold: INFO

logging:
  level: INFO
  appenders:
    - type: file
      archive: false
      currentLogFilename: var/log/atlasdb-ete.log

atlasdb:
  keyValueService:
    type: cassandra
    servers:
      - n1:9160
    ssl: false
    replicationFactor: 1
    keyspace: atlasete
